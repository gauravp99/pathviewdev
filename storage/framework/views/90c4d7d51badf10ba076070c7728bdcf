                    <?php

                    $pathidx = -1;
                    while (isset($_POST["pathway$pathidx"])) {
                        $pathidx++;
                    }

                    for ($pathnidx = 1; $pathnidx < $pathidx; $pathnidx++) {
                        echo "<input class=\"ex8\" list=\"pathwaylist\" name=\"pathway" . $pathnidx . " id=\"pathway" . $pathnidx . " value=" . $_POST["pathway$pathnidx"] . " >";

                    }


